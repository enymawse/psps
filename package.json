        "lint-staged": "^15.2.11",
        "prettier": "3.4.2",
        "typescript": "~5.6.2",
        "typescript-eslint": "8.18.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
